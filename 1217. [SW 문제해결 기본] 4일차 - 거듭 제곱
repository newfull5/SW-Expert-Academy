import java.util.Scanner;

class Solution{
	
	public static void main(String[] args){
		
		Scanner sc=new Scanner(System.in);
		
		for(int t=1;t<=10;t++){
			int test_case=sc.nextInt();
			System.out.printf("#%d %d\n",test_case,(int)Math.pow(sc.nextInt(),sc.nextInt()));		
		}				
	}//main end	
}//class end
